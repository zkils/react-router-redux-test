{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": ["jsdoc","closure"]
  },
  "source": {
    "include": ["src", "package.json", "README.md"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(node_modules/|docs/|doc_html)"
  },
  "plugins": [
    "plugins/markdown",
    "plugins/summarize"
  ],
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "theme": "superhero",
    "outputSourceFiles":true,
    "linenums":true,
    "navType":"vertical"
  },
  "opts": {
    "destination": "./docs/",
    "encoding": "utf8",
    "private": true,
    "recurse": true
  }
}